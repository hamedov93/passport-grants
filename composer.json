{
    "name": "hamedov/passport-grants",
    "description": "Custom grants for laravel passport",
    "keywords": ["laravel", "passport", "grant", "custom"],
    "require": {
        "php": "^8.0",
        "laravel/passport": "^11.0"
    },
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hamedov93/passport-grants/issues",
        "source": "https://github.com/hamedov93/passport-grants"
    },
    "authors": [
        {
            "name": "Mohamed Hamed",
            "email": "m.hamed@nezam.io"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Hamedov\\PassportGrants\\PassportGrantServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Hamedov\\PassportGrants\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
